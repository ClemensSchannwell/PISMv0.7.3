other_sources += iceRYANModel.cc pryan.cc

pryan : iceRYANModel.o iceEISModel.o pryan.o libpism.so
	${CLINKER} iceRYANModel.o iceEISModel.o pryan.o ${RYAN_LIB_FLAGS} ${ICE_LIB_FLAGS} -o $@


